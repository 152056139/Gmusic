<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.gray.gmusic.fragment.mine.MineFragment">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:ignore="UselessParent">

            <TableRow>

                <RelativeLayout
                    android:id="@+id/rl_one_one"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:layout_margin="3dp"
                    android:layout_weight="1"
                    android:background="#99999999">

                    <ImageView
                        android:id="@+id/iv_one_one"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/icon_local_music"
                        android:contentDescription="@string/main_activity_imageview_contentDescription_icon" />

                    <TextView
                        android:id="@+id/tv_one_one"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_one_one"
                        android:layout_margin="5dp"
                        android:text="我的音乐"
                        android:textColor="#ffffff" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_one_two"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:layout_margin="3dp"
                    android:layout_weight="1"
                    android:background="#99999999">

                    <ImageView
                        android:id="@+id/iv_one_two"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/icon_favorites"
                        android:contentDescription="@string/main_activity_imageview_contentDescription_icon" />

                    <TextView
                        android:id="@+id/tv_one_two"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_one_two"
                        android:layout_margin="5dp"
                        android:text="我的最爱"
                        android:textColor="#ffffff" />

                    <TextView
                        android:id="@+id/tv_one_two_count"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_below="@id/iv_one_two"
                        android:layout_margin="5dp"
                        android:text="0"
                        android:textColor="#ffffff" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_one_three"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:layout_margin="3dp"
                    android:layout_weight="1"
                    android:background="#99999999">

                    <ImageView
                        android:id="@+id/iv_one_three"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/icon_folder_plus" />

                    <TextView
                        android:id="@+id/tv_one_three"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_one_three"
                        android:layout_margin="5dp"
                        android:text="文件夹"
                        android:textColor="#ffffff" />

                    <TextView
                        android:id="@+id/tv_one_three_count"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_below="@id/iv_one_three"
                        android:layout_margin="5dp"
                        android:text="0"
                        android:textColor="#ffffff" />
                </RelativeLayout>

            </TableRow>

            <TableRow>

                <RelativeLayout
                    android:id="@+id/rl_two_one"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:layout_margin="3dp"
                    android:layout_weight="1"
                    android:background="#99999999">

                    <ImageView
                        android:id="@+id/iv_two_one"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/icon_artist_plus" />

                    <TextView
                        android:id="@+id/tv_two_one"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_two_one"
                        android:layout_margin="5dp"
                        android:text="歌手"
                        android:textColor="#ffffff" />

                    <TextView
                        android:id="@+id/tv_two_one_count"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_below="@id/iv_two_one"
                        android:layout_margin="5dp"
                        android:text="0"
                        android:textColor="#ffffff" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_two_two"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:layout_margin="3dp"
                    android:layout_weight="1"
                    android:background="#99999999">

                    <ImageView
                        android:id="@+id/iv_two_two"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/icon_album_plus" />

                    <TextView
                        android:id="@+id/tv_two_two"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_two_two"
                        android:layout_margin="5dp"
                        android:text="专辑"
                        android:textColor="#ffffff" />

                    <TextView
                        android:id="@+id/tv_two_two_count"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_below="@id/iv_two_two"
                        android:layout_margin="5dp"
                        android:text="0"
                        android:textColor="#ffffff" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_two_three"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:layout_margin="3dp"
                    android:layout_weight="1"
                    android:background="#99999999">

                    <ImageView
                        android:id="@+id/iv_two_three"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/icon_recent_play_plus" />

                    <TextView
                        android:id="@+id/tv_two_three"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_two_three"
                        android:layout_margin="5dp"
                        android:text="最近播放"
                        android:textColor="#ffffff" />

                    <TextView
                        android:id="@+id/tv_two_three_count"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_below="@id/iv_two_three"
                        android:layout_margin="5dp"
                        android:text="0"
                        android:textColor="#ffffff" />
                </RelativeLayout>

            </TableRow>

            <TableRow>

                <RelativeLayout
                    android:id="@+id/rl_three_one"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:layout_margin="3dp"
                    android:layout_weight="1"
                    android:background="#99999999">

                    <ImageView
                        android:id="@+id/iv_three_one"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/icon_genre_plus" />

                    <TextView
                        android:id="@+id/tv_three_one"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_three_one"
                        android:layout_margin="5dp"
                        android:text="风格"
                        android:textColor="#ffffff" />

                    <TextView
                        android:id="@+id/tv_three_one_count"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_below="@id/iv_three_one"
                        android:layout_margin="5dp"
                        android:text="0"
                        android:textColor="#ffffff" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_three_two"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:layout_margin="3dp"
                    android:layout_weight="1"
                    android:background="#99999999">

                    <ImageView
                        android:id="@+id/iv_three_two"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/icon_recent_add_plus" />

                    <TextView
                        android:id="@+id/tv_three_two"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_three_two"
                        android:layout_margin="5dp"
                        android:text="最近添加"
                        android:textColor="#ffffff" />

                    <TextView
                        android:id="@+id/tv_three_two_count"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_below="@id/iv_three_two"
                        android:layout_margin="5dp"
                        android:text="0"
                        android:textColor="#ffffff" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_three_three"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:layout_margin="3dp"
                    android:layout_weight="1"
                    android:background="#99999999">

                    <ImageView
                        android:id="@+id/iv_three_three"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/icon_new_playlist" />

                    <TextView
                        android:id="@+id/tv_three_three"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_three_three"
                        android:layout_margin="5dp"
                        android:text="新建列表"
                        android:textColor="#ffffff" />

                    <TextView
                        android:id="@+id/tv_three_three_count"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_below="@id/iv_three_three"
                        android:layout_margin="5dp"
                        android:text="0"
                        android:textColor="#ffffff" />
                </RelativeLayout>

            </TableRow>

            <TableRow>

                <RelativeLayout
                    android:id="@+id/rl_four_one"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:layout_margin="3dp"
                    android:layout_weight="1"
                    android:background="#99999999">

                    <ImageView
                        android:id="@+id/iv_four_one"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/icon_download" />

                    <TextView
                        android:id="@+id/tv_four_one"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_four_one"
                        android:layout_margin="5dp"
                        android:text="下载管理"
                        android:textColor="#ffffff" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_four_two"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:layout_margin="3dp"
                    android:layout_weight="1"
                    android:background="#99999999">

                    <ImageView
                        android:id="@+id/iv_four_two"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/icon_library_music_circle" />

                    <TextView
                        android:id="@+id/tv_four_two"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_four_two"
                        android:layout_margin="5dp"
                        android:text="音乐圈"
                        android:textColor="#ffffff" />


                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rl_four_three"
                    android:layout_width="match_parent"
                    android:layout_height="100dp"
                    android:layout_margin="3dp"
                    android:layout_weight="1"
                    android:background="#99999999">

                    <ImageView
                        android:id="@+id/iv_four_three"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/icon_home_custom" />

                    <TextView
                        android:id="@+id/tv_four_three"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/iv_four_three"
                        android:layout_margin="5dp"
                        android:text="定制首页"
                        android:textColor="#ffffff" />

                </RelativeLayout>

            </TableRow>

        </TableLayout>
    </ScrollView>

</FrameLayout>
